{
  "name": "family-calendar-card",
  "version": "0.0.1",
  "description": "Family Calendar Card",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "node build.js && rollup -c && cp -v dist/* /workspaces/core/config/www/community/family-calendar-card/",
    "watch": "rollup -c --watch",
    "watch:dev": "VERSION=`node -p \"require('./package.json').version\"` && TIMESTAMP=`TZ='America/New_York' date +%Y-%m-%d_%H:%M:%S` && echo Starting dev watch for version $VERSION && concurrently \"npm run watch\" \"onchange -d 1000 'dist/family-calendar-card.js' -- sh -c 'cp -vf dist/* /workspaces/core/config/www/community/family-calendar-card/ > /dev/null 2>&1 && echo New version built: $VERSION-dev.$TIMESTAMP'\""
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "custom-card-helpers": "^1.9.0",
    "lit": "^3.0.0",
    "lru-cache": "^10.0.0",
    "swiper": "^11.1.15"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.0.0",
    "@types/swiper": "^6.0.0",
    "concurrently": "^8.2.2",
    "home-assistant-js-websocket": "^9.1.0",
    "onchange": "^7.1.0",
    "rollup": "^4.0.0",
    "rollup-plugin-css-only": "^4.5.2",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  }
}
